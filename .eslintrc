{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "jest"
  ],
  "env": {
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "no-console": 0,
    "no-use-before-define": 0,
    "no-extra-boolean-cast": 0,
    "prettier/prettier": [
      "error"
    ]
  }
}
